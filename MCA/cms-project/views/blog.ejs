    <!-- NAVBAR JS  -->

    <script>
        document.addEventListener('DOMContentLoaded', function () {
            const personalTrainingBtn = document.getElementById('personal-training-btn');
            const megaDropdown = document.getElementById('mega-dropdown');
            const allNavItems = document.querySelectorAll('.nav-item');

            // Toggle dropdown on Personal Training click
            personalTrainingBtn.addEventListener('click', function (e) {
                e.preventDefault();
                e.stopPropagation();

                const isOpen = megaDropdown.classList.contains('show');

                // Close dropdown if open, open if closed
                if (isOpen) {
                    megaDropdown.classList.remove('show');
                    personalTrainingBtn.classList.remove('active');
                } else {
                    megaDropdown.classList.add('show');
                    personalTrainingBtn.classList.add('active');
                }
            });

            // Close dropdown when clicking on other nav items
            allNavItems.forEach(item => {
                item.addEventListener('click', function () {
                    megaDropdown.classList.remove('show');
                    personalTrainingBtn.classList.remove('active');
                });
            });

            // Close dropdown when clicking anywhere else
            document.addEventListener('click', function (e) {
                if (!e.target.closest('.mega-menu')) {
                    megaDropdown.classList.remove('show');
                    personalTrainingBtn.classList.remove('active');
                }
            });

            // Prevent dropdown from closing when clicking inside it
            megaDropdown.addEventListener('click', function (e) {
                e.stopPropagation();
            });
        });
    </script>

    <!-- HERO SECTION  -->

    <script>
        const tabs = document.querySelectorAll('.blgtab');
        const centerImage = document.getElementById('blgcenter-image');
        const mainDescription = document.getElementById('blgmain-description');
        const mainHeading = document.getElementById('blgmain-heading');

        const tabData = {
            'healthy-fats': {
                heading: 'Healthy Fats and Oils',
                image: 'https://images.unsplash.com/photo-1540420773420-3366772f4999?w=400&h=400&fit=crop&crop=center',
                description: 'The general opinion that fats are basically bad and only make you fat has ensured that more and more people are eating less and less fat. The result of a low-fat diet is surprising and should make you think.'
            },
            'body-need': {
                heading: 'Why Does The Body Need Healthy Fats?',
                image: 'https://images.unsplash.com/photo-1512621776951-a57141f2eefd?w=400&h=400&fit=crop&crop=center',
                description: 'Essential fatty acids are crucial for proper body function. They help absorb fat-soluble vitamins A, D, E, and K, support brain health, and maintain healthy cell membranes throughout your body.'
            },
            'unhealthy-fats': {
                heading: 'What Fats Are Unhealthy?',
                image: 'https://images.unsplash.com/photo-1571091718767-18b5b1457add?w=400&h=400&fit=crop&crop=center',
                description: 'Trans fats and hydrogenated oils are among the most dangerous fats for human health. These artificially modified fats increase inflammation, raise bad cholesterol, and significantly increase the risk of heart disease.'
            },
            'healthy-processing': {
                heading: 'Healthy Fat Processing',
                image: 'https://images.unsplash.com/photo-1490645935967-10de6ba17061?w=400&h=400&fit=crop&crop=center',
                description: 'Cold-pressed oils and minimally processed fats retain their nutritional value and beneficial properties. Gentle processing methods preserve antioxidants, vitamins, and essential fatty acids that support optimal health.'
            }
        };

        tabs.forEach(tab => {
            tab.addEventListener('click', () => {
                // Remove active class from all tabs
                tabs.forEach(t => t.classList.remove('blgactive'));

                // Add active class to clicked tab
                tab.classList.add('blgactive');

                // Get tab data
                const tabKey = tab.getAttribute('data-tab');
                const data = tabData[tabKey];

                // Add rotating effect to image
                centerImage.classList.add('blgrotating');

                // Update content with smooth transition
                centerImage.style.opacity = '0';
                mainDescription.style.opacity = '0';
                mainHeading.style.opacity = '0';

                setTimeout(() => {
                    centerImage.src = data.image;
                    mainDescription.textContent = data.description;
                    mainHeading.textContent = data.heading;

                    centerImage.style.opacity = '1';
                    mainDescription.style.opacity = '1';
                    mainHeading.style.opacity = '1';

                    // Remove rotating class after animation
                    setTimeout(() => {
                        centerImage.classList.remove('blgrotating');
                    }, 500);
                }, 250);
            });
        });

        // Read more button functionality
        document.querySelector('.blgread-more-btn').addEventListener('click', () => {
            alert('Read more functionality would navigate to detailed article page');
        });
    </script>

